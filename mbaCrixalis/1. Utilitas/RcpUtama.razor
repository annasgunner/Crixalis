@page "/"
@using Microsoft.AspNetCore.Components
@using rclPantheon.Utility
@inherits PthRcpUtama

@{
	base.BuildRenderTree(__builder);
}
@code {
    protected override async void OnInitialized() {
        var components = Assembly.GetExecutingAssembly().ExportedTypes.Where(t => t.IsSubclassOf(typeof(ComponentBase))).ToList().Where(c => c.Namespace.Contains("Pages"));

        var component = components.FirstOrDefault(r => r.Name == "Counter");
        pathForm = component;
        var a = component.Attributes;
        var b = component.GetMembers();
        base.OnInitialized();
    }
    public override void cbxCari_Enter(KeyboardEventArgs e)
    {
        base.cbxCari_Enter(e);
    }
    public override void cbxCari_OnInput(ChangeEventArgs e)
    {
        base.cbxCari_OnInput(e);
    }
}